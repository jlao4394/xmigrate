{
  "name": "@rxdi/xmigrate",
  "version": "0.7.12",
  "main": "index.js",
  "author": "Kristiyan Tachev",
  "license": "MIT",
  "scripts": {
    "build": "tsc && gapi build",
    "lint": "tslint -c tslint.json 'development/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxdi/xmigrate"
  },
  "bin": {
    "xmigrate": "./dist/main.js"
  },
  "dependencies": {
    "@rxdi/core": "^0.7.11",
    "chalk": "^2.4.2",
    "date-fns": "^1.30.1",
    "esm": "^3.2.25",
    "mongodb": "^3.3.0-beta2",
    "mongoose": "^5.6.6"
  },
  "devDependencies": {
    "@rxdi/parcel-plugin-shebang": "0.7.5",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^24.0.15",
    "@types/mongodb": "^3.1.30",
    "@types/mongoose": "^5.5.9",
    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "./jest-setup.ts"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/*/.*\\.spec.(ts|tsx|js)$",
    "verbose": true,
    "collectCoverage": true
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts"
}
